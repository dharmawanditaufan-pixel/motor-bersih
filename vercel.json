{
  "version": 2,
  "name": "motor-bersih-pos",
  "builds": [
    {
      "src": "api/**/*.php",
      "use": "@vercel/php"
    },
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "DB_HOST": "@db_host",
    "DB_NAME": "@db_name",
    "DB_USER": "@db_user",
    "DB_PASS": "@db_pass"
  },
  "regions": ["sin1"]
}
